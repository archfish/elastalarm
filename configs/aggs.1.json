{
    "title": "percent",
    "interval": "2s",
    "index": "logstash-nginx-access-*",
    "paradigm": "percentage",
    "condition": {
        "part": {
            "prefix": {
                "retcode.keyword": "5"
            }
        },
        "whole": {
            "range": {
                "@timestamp": {
                    "gt": "now-30d"
                }
            }
        },
        "match": {
            "gt": 0.02
        }
    }
}